# Service Dashboard Environment Configuration
# Copy this file to .env and fill in your actual values

# TOKENS AND SECRETS
SECRET_KEY=your-secret-key-here
GITHUB_TOKEN=your-github-token-here
GITLAB_TOKEN=your-gitlab-token-here

# USERNAMES
GITHUB_USERNAME=your-github-username
GITLAB_USERNAME=your-gitlab-username

# JIRA CONFIGURATION
JIRA_PROJECT=RHCLOUD
JIRA_PERSONAL_ACCESS_TOKEN=your-jira-token-here

# DEPLOYMENT CONFIGURATION
# Comma-separated list of deployment name substrings to ignore during data download
DEPLOY_TEMPLATE_IGNORE_LIST=

# Comma-separated list of deployment name mappings (old:new)
DEPLOYMENT_RENAME_LIST=

# APP-INTERFACE CONFIGURATION
# Comma-separated list of GitLab usernames to track
APP_INTERFACE_USERS=

# PLUGINS
# Enable BackOffice Proxy plugin (true/false)
ENABLE_BACKOFFICE_PROXY_PLUGIN=false

# OpenShift tokens for BackOffice Proxy plugin
# Get from: https://oauth-openshift.apps.mpp-w2-prod.0jgd.p1.openshiftapps.com/oauth/token/request (prod)
#           https://oauth-openshift.apps.mpp-w2-preprod.cfln.p1.openshiftapps.com/oauth/token/request (stage)
OPENSHIFT_TOKEN_PROD=sha256~your-openshift-prod-token-here
OPENSHIFT_TOKEN_STAGE=sha256~your-openshift-stage-token-here

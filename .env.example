# Flask secret key
SECRET_KEY=your-secret-key-here

# GitHub configuration (required)
GITHUB_TOKEN=your-github-token
GITHUB_USERNAME=your-github-username

# GitLab configuration (required)
GITLAB_TOKEN=your-gitlab-token-with-api-scope
GITLAB_USERNAME=your-gitlab-username

# JIRA configuration (required)
JIRA_PERSONAL_ACCESS_TOKEN=your-jira-token
JIRA_PROJECT=your-jira-project-key

# App-interface users (comma-separated)
APP_INTERFACE_USERS=user1,user2,user3

# Deployment configuration (optional)
DEPLOY_TEMPLATE_IGNORE_LIST=template1,template2
DEPLOYMENT_RENAME_LIST=old-name:new-name,another-old:another-new
